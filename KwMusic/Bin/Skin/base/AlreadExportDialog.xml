<?xml version="1.0" encoding="UTF-8"?>
<Window size="300,113" caption="0,0,0,32" roundcorner="10,10">
	<EventHook event="OnSkinChange">
		<Attribute control="text_title" value="textcolor=&quot;PopWnd.title.textcolor&quot;" />
	</EventHook>
	<VerticalLayout name="layout_clientrect">
		<HorizontalLayout name="layout_dialog_title" height="32">
			<Label name="text_title" padding="8,0,0,0" align="left" font="Main.ChannelTitle" textcolor="#ff333333" text="酷我提示" />
			<Control />
			<Button name="downexport_Close" padding="0,10,0,10" width="19" height="20" statusimage="downexport_Close.png"/>
		</HorizontalLayout>
		<HorizontalLayout name="layout_text" height="20" padding="0,15,0,0">
			<Control />
			<Label name="text_text1" varwidth="true"  align="left" text="" font="Default.FontName" textcolor="#ff333333" layertext="true"/>
			<Control />
		</HorizontalLayout> 
		<HorizontalLayout name="layout_opbutton" height="20" padding="0,20,0,0">
			<Control />
			<Button  name="button_opendir" align="left" textcolor="#ff0096d2" text="打开目录" padding="10,0,0,0" width="58" height="15" font="Default.FontName" setcursor="true" layertext="true"/> 
			<Label bkcolor="#ff999999" padding="5,2,0,0" width="1" height="12"/>
			<Button  name="button_exit" align="left" textcolor="#ff0096d2" text="取消" padding="10,0,0,0" width="58" height="15" font="Default.FontName" setcursor="true" layertext="true"/>
			<Control />
		</HorizontalLayout> 	
	</VerticalLayout>
</Window>